<% if @poll.present? %>
  <%= back_link_to admin_poll_path(@poll) %>
<% else %>
  <%= back_link_to %>
<% end %>

<h2 class="margin-top">
  <% if @poll.present? %>
    <%= t("admin.questions.new.title", poll: @poll.name) %>
  <% else %>
    <%= t("admin.questions.new.title_proposal") %>
  <% end %>
</h2>

<div class="poll-question-form">
  <%= render "form", form_url: admin_questions_path %>
</div>
